(in-package #:org.shirakumo.lichat.protocol)

#+sbcl
(defun generate-valid-name-char-p ()
  (format T "(or (= #x0020 cp)")
  (loop with b = NIL
        for i from #x0000 to #x10FFFF
        for cat = (sb-unicode:general-category (code-char i))
        for visible = (find (char (string cat) 0) "LMNPS")
        do (cond ((and b (not visible))
                  (if (= b (1- i))
                      (format T "~&(= #x~4,'0x cp)" b)
                      (format T "~&(<= #x~4,'0x cp #x~4,'0x)" b (1- i)))
                  (setf b NIL))
                 ((and (not b) visible)
                  (setf b i)))
        finally (when b (format T "~&(<= #x~4,'0x cp #x~4,'0x)" b (1- i))))
  (format T ")"))

#+sbcl
(defun valid-name-char-p (c)
  (or (char= #\Space c)
      (find (char (symbol-name (sb-unicode:general-category c)) 0) "LMNPS")))

